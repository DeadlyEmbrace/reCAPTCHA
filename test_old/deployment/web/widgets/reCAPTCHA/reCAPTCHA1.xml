<?xml version="1.0" encoding="utf-8" ?>
<widget id="reCAPTCHA.widget.reCAPTCHA1" needsEntityContext="true" xmlns="http://www.mendix.com/widget/1.0/">
    <name>reCAPTCHA1</name>
    <description>Protect your Mendix app from spam and abuse while letting real people pass through with ease.</description>

    <icon></icon>

    <properties>
        <property key="btnName" type="string" required="true" defaultValue="">
            <caption>Name</caption>
            <category>Action Button</category>
            <description>The name of the Mendix button that submits the form.</description>
        </property>
        <property key="themeString" type="enumeration" required="true" defaultValue="clean">
            <caption>Theme</caption>
            <category>Appearance</category>
            <description>Defines which theme to use for reCAPTCHA.</description>
            <enumerationValues>
                <enumerationValue key="red">Red</enumerationValue>
                <enumerationValue key="white">White</enumerationValue>
                <enumerationValue key="blackglass">Black Glass</enumerationValue>
                <enumerationValue key="clean">Clean</enumerationValue>
            </enumerationValues>
        </property>
        <property key="langString" type="enumeration" required="true" defaultValue="en">
            <caption>Language</caption>
            <category>Appearance</category>
            <description>Which language is used in the interface for the pre-defined themes.</description>
            <enumerationValues>
                <enumerationValue key="en">English</enumerationValue>
                <enumerationValue key="nl">Dutch</enumerationValue>
                <enumerationValue key="fr">French</enumerationValue>
                <enumerationValue key="de">German</enumerationValue>
                <enumerationValue key="pt">Portuguese</enumerationValue>
                <enumerationValue key="ru">Russian</enumerationValue>
                <enumerationValue key="es">Spanish</enumerationValue>
                <enumerationValue key="tr">Turkish</enumerationValue>
            </enumerationValues>
        </property>
        <property key="tabIndexInteger" type="integer" required="true" defaultValue="0">
            <caption>Tabindex</caption>
            <category>Appearance</category>
            <description>The tabindex of the widget and challenge. If other elements in your page use tabindex, it should be set to make user navigation easier.</description>
        </property>
        <property key="challengeAttribute" type="attribute" required="true">
            <caption>Challenge</caption>
            <category>Entity Context</category>
            <description>The recaptcha challenge to verify server side.</description>
            <attributeTypes>
                <attributeType name="String"/>
            </attributeTypes>
        </property>
        <property key="responseTokenAttribute" type="attribute" required="true">
            <caption>Response</caption>
            <category>Entity Context</category>
            <description>The recaptcha response to verify server side.</description>
            <attributeTypes>
                <attributeType name="String"/>
            </attributeTypes>
        </property>
        <property key="siteKeyEntity" type="entity" allowNonPersistableEntities="true" required="false" defaultValue="">
            <caption>Sitekey Entity</caption>
            <category>Sitekey</category>
            <description></description>
        </property>
        <property key="siteKeyAttribute" type="attribute" entityProperty="siteKeyEntity" required="false" defaultValue="">
            <caption>Sitekey attribute</caption>
            <category>Sitekey</category>
            <description></description>
            <attributeTypes>
                <attributeType name="String" />
            </attributeTypes>
        </property>
        <property key="mfGetSiteKey" type="microflow" required="false" defaultValue="">
            <caption>Sitekey Microflow</caption>
            <category>Sitekey</category>
            <description></description>
            <returnType type="Object" entityProperty="siteKeyEntity" />
        </property>
    </properties>    
</widget>